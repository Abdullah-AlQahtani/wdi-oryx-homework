<%= form_for @employee do |f| %>
<div>
<%= f.label :name %>
<%= f.text_field :name %>
</div>
<div>
<%= f.label :age %>
<%= f.text_field :age %>
</div>
<div>
<%= f.label :gender %>
<%= f.text_field :gender %>
</div>
<div>
<%= f.label :company_name %>
 <%=f.collection_select(:company_id, Company.all, :id, :name) %>
</div>
<div>
<%= f.submit %>
</div>
<% end %>